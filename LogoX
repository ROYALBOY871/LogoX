@bot.on(events.NewMessage(pattern="^/start"))
async def start(event):
         Reply = f"ᴛᴀᴅᴀ ! ɪ'ᴍ ʜᴇʀᴇ ꜰᴏʀ ʜᴇʟᴘɪɴɢ ʏᴏᴜ ᴡɪᴛʜ ꜱᴏᴍᴇ ᴀᴡᴇꜱᴏᴍᴇ ᴄᴏᴍᴍᴀɴᴅ ꜰᴏʀ ᴍᴀᴋɪɴɢ ᴘʀᴏ ʟᴏɢᴏ ᴄʟɪᴄᴋ ʙᴇʟᴏᴡ ʙᴜᴛᴛᴏɴ🥀"
keyboard = [
      [  
        Button.inline("↑ᴘʀᴏ ʟᴏɢᴏ ᴏʀᴅᴇʀ✓", data="↑ᴘʀᴏ ʟᴏɢᴏ ᴏʀᴅᴇʀ✓")
        ]
await x.send_message(f"
!!! ꜱᴇɴᴅ ᴍᴇ ʙᴀᴄᴋɢʀᴏᴜɴᴅ ᴊᴘɢ↓↓↓", buttons=keyboard)
bot.conversation(event.chat_id) as x:
      await x.send_media("Thanks for using our service !!!! ꜰᴏʀ ɪɴᴛʀᴏ ᴠɪᴅᴇᴏ ᴄᴏɴᴛᴀᴄᴛ - @anoxD 1 TRX = 1 intro video... ")

    
         
